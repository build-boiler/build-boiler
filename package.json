  },
  "bugs": {
    "url": "https://github.com/dtothefp/build-boiler/issues"
  },
  "engines": {
    "node": ">=5.0.0"
  },
  "keywords": [
    "assemble",
    "browser-sync",
    "eslint",
    "gulp",
    "nunjucks",
    "react",
    "webpack"
  ],
  "devDependencies": {
    "async": "^2.0.0-rc.1",
