{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "arrowFunctions": true,
      "blockBindings": true,
      "experimentalObjectRestSpread": true,
      "generators": true,
      "jsx": true,
      "module": true,
      "modules": true,
      "restParams": true,
      "spread": true,
      "jsx": true
    }
  },
  "globals": {
    "Promise": true,
    "Map": true,
    "describe": true,
    "it": true,
    "before": true,
    "after": true,
    "beforeEach": true,
    "afterEach": true,
    "expect": true,
    "should": true,
    "$": true,
    "jQuery": true,
    "Modernizr": true,
    "_fbq": true,
    "FB": true,
    "ga": true,
    "templateVars": true,
    "_usq": true,
    "optimizely": true
  }
}
